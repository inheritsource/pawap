      </extensionElements>
    </startEvent>
    <userTask id="handlaggare1" name="Handläggare" activiti:candidateUsers="admin" activiti:candidateGroups="Handlaggare">
      <extensionElements>
        <activiti:taskListener event="complete" class="org.inheritsource.service.delegates.CreateVariablesTaskListener"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="handlaggare1">
      <extensionElements>
        <activiti:executionListener event="take" class="org.inheritsource.service.delegates.CreateVariablesFlowListener"></activiti:executionListener>
      </extensionElements>
    </sequenceFlow>
    <scriptTask id="scripttask1" name="Map to group and prepare email" scriptFormat="groovy" activiti:autoStoreVariables="false">
      <script>
// read the used variables. Otherwise there will be problems if they are not set.
execution.setVariable("felanmalan_first_name", execution.getVariable("startevent1_first_name")) 
execution.setVariable("felanmalan_address_string", execution.getVariable("startevent1_address_string")) 
execution.setVariable("felanmalan_service_code", execution.getVariable("startevent1_service_code")) 
execution.setVariable("felanmalan_email", execution.getVariable("startevent1_email")) 
execution.setVariable("felanmalan_Handlggare_comment", execution.getVariable("Handlggare_comment")) 
println("Hello world!"); 
String group ; 
if ( Handlggare_group.equals("two") )
  // { group = "elektriker" ; } 
  { group = "Registrator" ; } 
else if ( Handlggare_group.equals("three") )
  { group = "Assistent" ; }
else if ( Handlggare_group.equals("four") )
  { group = "Handlaggare" ; }
else 
  { group = "admin" ; }
execution.setVariable("group" , group  ) ; 
println("group :group =" + group ) ;
def recipientEmail= felanmalan_email   ;
from = "noreply@motrice.se" ; 
if ( felanmalan_service_code == null)
  {
  felanmalan_service_code = "Okänd service_code"
  }
if ( felanmalan_address_string == null)
  {
  felanmalan_address_string = "Okänd adress"
  }
if ( felanmalan_Handlggare_comment == null)
  {
  felanmalan_Handlggare_comment = ""
  }
messageText = "Ditt Ärende på "+felanmalan_address_string+"(service_code=" + felanmalan_service_code + "  är utfört."+ felanmalan_Handlggare_comment ; 
messageSubject = "Ärende utfört" ; 
execution.setVariable("from" , from  ) ; 
execution.setVariable("recipientEmail" , recipientEmail  ) ; 
execution.setVariable("messageText" , messageText  ) ; 
execution.setVariable("messageSubject" , messageSubject  ) ; 
println("done ...") ; 
</script>
    </scriptTask>
    <sequenceFlow id="flow2" sourceRef="handlaggare1" targetRef="scripttask1"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway" default="flow4"></exclusiveGateway>
    <sequenceFlow id="flow3" sourceRef="scripttask1" targetRef="exclusivegateway1"></sequenceFlow>
    <userTask id="usertask2" name="Utförare" activiti:candidateUsers="admin" activiti:candidateGroups="${group}"></userTask>
    <sequenceFlow id="flow4" sourceRef="exclusivegateway1" targetRef="usertask2"></sequenceFlow>
    <scriptTask id="scripttask2" name="Prepare email for rejection" scriptFormat="groovy" activiti:autoStoreVariables="false">
      <script>String messageText2 = "Ditt Ärende på"+felanmalan_address_string+"(service_code=" + felanmalan_service_code + "  kommer inte att åtgärdas."+felanmalan_Handlggare_comment ; 
               messageSubject = "Ärende " ; 
               execution.setVariable("messageSubject" , messageSubject  ) ; 
               println("messageText2 :messageText2 =" + messageText2 ) ;
println("script2") ; 
               execution.setVariable("messageText" , messageText2  ) ; 
     </script>
    </scriptTask>
    <sequenceFlow id="flow5" sourceRef="exclusivegateway1" targetRef="scripttask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${Handlggare_group== 'one'}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow6" sourceRef="usertask2" targetRef="exclusivegateway2"></sequenceFlow>
    <sequenceFlow id="flow7" sourceRef="scripttask2" targetRef="exclusivegateway2"></sequenceFlow>
    <serviceTask id="servicetask1" name="Email " activiti:class="org.inheritsource.service.delegates.GenericServiceMessageDelegate">
      <extensionElements>
        <activiti:field name="recipientEmail">
          <activiti:expression>${recipientEmail}</activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:expression>${from}</activiti:expression>
        </activiti:field>
        <activiti:field name="messageText">
          <activiti:expression>${messageText}</activiti:expression>
        </activiti:field>
        <activiti:field name="messageSubject">
          <activiti:expression>${messageSubject}</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow8" sourceRef="exclusivegateway2" targetRef="servicetask1"></sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow9" sourceRef="servicetask1" targetRef="endevent1"></sequenceFlow>
  </process>
